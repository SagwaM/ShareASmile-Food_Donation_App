{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" },
      { "source": "/api/(.*)", "destination": "https://shareasmile-food-donation-app.onrender.com/api/$1" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "no-cache" }
        ]
      }
    ]
  }
  